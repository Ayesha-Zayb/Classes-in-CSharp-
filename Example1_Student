 Example1_Student/Student.cs
using System;

class Student
{
    public int Id;
    public string Name;

    public void DisplayInfo()
    {
        Console.WriteLine("Student ID: " + Id);
        Console.WriteLine("Student Name: " + Name);
    }
}

class Program
{
    static void Main()
    {
        Student s1 = new Student();
        s1.Id = 101;
        s1.Name = "Ayesha";
        s1.DisplayInfo();
    }
}
